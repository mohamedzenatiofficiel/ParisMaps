{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/static/css/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/static/js/scripts.js",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/travaux",
      "dest": "/api/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}
